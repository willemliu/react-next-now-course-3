{
    "name": "course-3",
    "version": 2,
    "alias": ["dev-course-3.willemliu.now.sh"],
    "build": {
        "env": {
            "SECRET": "@course-3-dev-secret",
            "PUBLIC_SECRET": "@course-3-dev-public-secret"
        }
    },
    "routes": [
        {
            "src": "/(?<section>.+)/(?<id>\\d+)",
            "dest": "/index?section=$section&id=$id"
        }
    ]
}
